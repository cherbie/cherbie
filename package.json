    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.55.1",
    "svelte-check": "^2.9.2",
    "svelte-loader": "^3.1.4",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "@steeze-ui/octicons": "^2.0.1",
    "@steeze-ui/svelte-icon": "^1.3.2"
